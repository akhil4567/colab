{
    "name": "unleashed-backend",
    "version": "1.0.0",
    "description": "api routes database models, migraitons & configs.",
    "main": "index.ts",
    "scripts": {
        "clean": "rm -rf ./build/*",
        "clean_windows": "del .\\build\\*",
        "start-local": "ts-node-dev src/apis/index.ts",
        "start": "node build/index.js",
        "lint": "tsc --noEmit -p .",
        "build": "npm run clean && cp .env ./build/.env && tsc",
        "build_win": "npm run clean_windows && copy .env ./build/.env && tsc",
        "generate_migration": "npx sequelize-cli migration:generate --name",
        "make_migration": "npx sequelize-cli db:migrate",
        "undo_migration": "npx sequelize-cli db:migrate:undo --name",
        "create_seed": "npx sequelize-cli db:seed:all"
    },
    "author": "",
